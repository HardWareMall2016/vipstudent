<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    <declare-styleable name="SwitchButton">
        <attr name="bmWidth" format="dimension"></attr>
        <attr name="bmHeight" format="dimension"></attr>
    </declare-styleable>
    <declare-styleable name="WheelView">
        <attr name="divierDrawable" format="reference" />
        <attr name="mainTextColor" format="color" />
        <attr name="mainTextSize" format="dimension" />
        <attr name="secondTextColor" format="color" />
        <attr name="secondTextSize" format="dimension" />
        <attr name="thirdTextColor" format="color" />
        <attr name="thirdTextSize" format="dimension" />
        <attr name="lableTextColor" format="color" />
        <attr name="lableTextSize" format="dimension" />
        <attr name="centerDrawable" format="reference" />
        <attr name="shadowColor" format="color" />
    </declare-styleable>
    <declare-styleable name="RatingBar">
        <!-- 不可用图片 -->
        <attr name="disable_star" format="reference" />
        <!-- 空图片 -->
        <attr name="empty_star" format="reference" />
        <!-- 一半的图片 -->
        <attr name="half_star" format="reference" />
        <!-- 完全的图片 -->
        <attr name="full_star" format="reference" />
        <!-- 显示个数 -->
        <attr name="numStars" format="integer" />
        <!-- 等级 -->
        <attr name="rating" format="float" />
        <!-- 是否是指示器 -->
        <attr name = "isIndicator" format = "boolean" />
        <!-- 每个星星的间距 -->
        <attr name="startsPadding" format="dimension" />
        <!-- 是否可用-->
        <attr name = "enable" format = "boolean" />
    </declare-styleable>
</resources>